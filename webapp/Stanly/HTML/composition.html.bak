							<script type="text/javascript">
							nv.addGraph(function(){
								var chart = nv.models.pie()
									.values(function(d) { return d })
									.width(200)
									.height(200)
									.color([ "#EE0000", "#F77700", "#fdd04f", "#00AA00" ])
									.donut(true)
									.donutRatio(0.5);

								d3.select("#project-pol")
									.datum([[
										{ 
										  key: "One",
										  y: 5.5
										},
										{ 
										  key: "Two",
										  y: 3
										},
										{ 
										  key: "Three",
										  y: 2.5
										},
										{ 
										  key: "Four",
										  y: 1.5
										},
									 ]])
									.transition().duration(1200)
									.attr('width', 200)
									.attr('height', 200)
									.call(chart);

								return chart;
							});
							</script>
